
local adminCmd = {"AK_ADMEN1", "328ml", "GYATT_DAMN1","IIIlIIIllIlIllIII","AliKhammas1234","dgthgcnfhhbsd","AliKhammas", "YournothimbuddyXD"}


if game.ReplicatedStorage:FindFirstChild('DefaultChatSystemChatEvents') then
    oldChat = true
end
local function Chat(msg)
    if oldChat then
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(msg, "All")
    else
        game.TextChatService.TextChannels.RBXGeneral:SendAsync(msg)
    end
end


